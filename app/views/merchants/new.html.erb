<form action="/merchants" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter Merchant Information:</p>

  <span>Merchant Name</span>
  <input type='text' name='merchant[name]'/><br/>
  <span>Merchant Street Address</span>
  <input type='text' name='merchant[address]'/><br/>
  <input type='text' name='merchant[city]'/><br/>
  <input type='text' name='merchant[state]'/><br/>
  <input type='text' name='merchant[zipcode]'/><br/>

  <input type='submit'/>
</form>
